{
    "preset": "fastnet",
    "poet": {
        "phase-shift": "30s",
        "cycle-gap": "30s",
        "grace-period": "10s"
    },
    "tortoise": {
        "tortoise-enable-tracer": true
    },
    "api": {
        "grpc-public-listener": "0.0.0.0:9092",
        "grpc-private-listener": "0.0.0.0:9093"
    },
    "logging": {
        "hare": "debug"
    },
    "fetch": {
        "servers-metrics": true
    },
    "smeshing": {
        "smeshing-verifying-opts": {
            "smeshing-opts-verifying-min-workers": 1000000
        }
    }
}