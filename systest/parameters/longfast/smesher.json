{
    "preset": "fastnet",
    "main": {
        "layers-per-epoch": 100,
        "layer-duration": "18s"
    },
    "poet": {
        "phase-shift": "10m",
        "cycle-gap": "1m",
        "grace-period": "5s",
        "positioning-atx-selection-timeout":"3s"
    },
    "tortoise": {
        "tortoise-zdist": 4,
        "tortoise-hdist": 10
    },
    "hare": {
        "hare-wakeup-delta": "5s",
        "hare-round-duration": "2s",
        "hare-limit-iterations": 4
    },
    "hare-eligibility": {
        "eligibility-confidence-param": 10
    },
    "beacon": {
        "beacon-grace-period-duration": "10s",
        "beacon-proposal-duration": "10s",
        "beacon-first-voting-round-duration": "10s",
        "beacon-rounds-number": 10,
        "beacon-voting-round-duration": "10s",
        "beacon-weak-coin-round-duration": "10s"
    },
    "smeshing": {
        "smeshing-verifying-opts": {
            "smeshing-opts-verifying-min-workers": 1000000
        }
    },
    "certifier": {
        "client": {
            "max-retries": 10
        }
    },
    "logging": {
        "log-encoder": "json"
    }
}
